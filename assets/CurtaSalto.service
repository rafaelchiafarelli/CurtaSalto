[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
WorkingDirectory=/home/skiafa/workspace/CurtaSaltoD/CurtaSalto
ExecStart=/home/skiafa/workspace/CurtaSaltoD/venv/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/home/skiafa/workspace/CurtaSaltoD/CurtaSalto/CurtaSalto.sock \
          CurtaSalto.wsgi:application

[Install]
WantedBy=multi-user.target
